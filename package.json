{
  "name": "react-webpack-boilerplate",
  "version": "2.0.0",
  "description": "Simple production ready boilerplate for React, Webpack (SASS and React hot reloading), tests and coverage.",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "dist": "cross-env NODE_ENV=production webpack -p --progress",
    "postinstall": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/srn/react-webpack-boilerplate.git"
  },
  "engines": {
    "node": "5",
    "npm": "3"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "production",
    "sass",
    "hot",
    "reload"
  ],
  "author": "Søren Brokær <root@srn.io> (http://srn.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/srn/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/srn/react-webpack-boilerplate",
  "dependencies": {
    "babel-polyfill": "6.3.14",
    "babel-runtime": "6.3.19",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "draft-js": "~0.10.0",
    "draft-js-plugins-editor": "file:./draft-js-plugins-editor",
    "draft-js-hashtag-plugin": "1.1.0",
    "draft-js-inline-toolbar-plugin": "2.0.0-beta6",
    "draft-js-linkify-plugin": "1.0.1",
    "draft-js-richbuttons-plugin": "1.1.0",
    "draft-js-side-toolbar-plugin": "2.0.0-beta9",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "webpack": "1.12.12"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.2.0",
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-runtime": "6.4.3",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "css-loader": "0.23.1",
    "node-sass": "3.4.2",
    "react-addons-test-utils": "15.2.1",
    "react-hot-loader": "1.3.1",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "webpack-dev-server": "~1.16.3"
  }
